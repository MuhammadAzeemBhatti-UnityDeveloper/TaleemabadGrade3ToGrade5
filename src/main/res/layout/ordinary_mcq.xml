        android:layout_height="0dp"
        android:gravity="center_horizontal|top"
        android:typeface="normal"
        android:scrollbars = "vertical"
        app:layout_constraintBottom_toTopOf="@id/guidelineQuestionBottom"
        app:layout_constraintEnd_toStartOf="@+id/guidelineQuestionRight"
        app:layout_constraintStart_toEndOf="@id/guidelineQuestionLeft"
        app:layout_constraintTop_toTopOf="@+id/guidelineQuestionTop" />

    <androidx.constraintlayout.widget.Guideline
