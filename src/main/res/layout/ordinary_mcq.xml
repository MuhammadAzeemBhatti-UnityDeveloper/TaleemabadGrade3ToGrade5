        android:layout_height="0dp"
        android:lineSpacingExtra="5sp"
        android:gravity="center"
        android:paddingLeft="15dp"
        android:paddingRight="15dp"
        android:paddingBottom="5dp"
        android:typeface="normal"
        app:layout_constraintBottom_toTopOf="@id/guidelineOption1Bottom"
        app:layout_constraintEnd_toStartOf="@+id/guidelineOption1Right"
        app:layout_constraintStart_toEndOf="@id/guidelineOption1Left"
        app:layout_constraintTop_toTopOf="@+id/guidelineOption1Top" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineOption2Left"
