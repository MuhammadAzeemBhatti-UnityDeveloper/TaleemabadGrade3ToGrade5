        app:layout_constraintGuide_percent=".121" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineOption1Right"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_percent=".494" />


    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guidelineOption1Top"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
